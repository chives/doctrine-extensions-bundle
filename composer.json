{
    "name": "fsi/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi Doctrine extensions bundle.",
    "keywords": ["fsi", "bundle", "doctrine", "extensions", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "twig/twig": "^1.34|^2.0",
        "symfony/framework-bundle": "^2.4|^3.0",
        "symfony/twig-bundle": "^2.4|^3.0",
        "symfony/http-foundation": "^2.4|^3.0",
        "symfony/options-resolver": "^2.6|^3.0",
        "symfony/form" : "^2.4|^3.0",
        "symfony/validator": "^2.4|^3.0",
        "doctrine/doctrine-bundle": "^1.2",
        "fsi/doctrine-extensions": "^2.0@dev",
        "sensio/framework-extra-bundle": "^3.0",
        "knplabs/knp-gaufrette-bundle": "~0.1"
    },
    "require-dev": {
        "bossa/phpspec2-expect": "^2.3",
        "doctrine/orm": "^2.5",
        "fsi/datagrid-bundle": "^1.1|^2.0@dev",
        "phpunit/phpunit": "^4.8|^5.7",
        "phpspec/phpspec": "^3.0",
        "symfony/asset": "^2.7|^3.0",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/security": "^2.4|^3.0",
        "symfony/twig-bridge": "^2.4|^3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\DoctrineExtensionsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "FSi\\Bundle\\DoctrineExtensionsBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
